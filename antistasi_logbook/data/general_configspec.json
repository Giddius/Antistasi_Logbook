{
    "sections": {
        "general": {
            "entries": {
                "max_threads": {
                    "converter": "integer",
                    "default": "5",
                    "verbose_name": "Maximum amount of Threads",
                    "description": "How many Threads should the App maximal use"
                },
                "is_first_start": {
                    "converter": "boolean",
                    "default": "False",
                    "gui_visible": false
                }
            }
        },
        "database": {
            "entries": {
                "database_path": {
                    "converter": "path",
                    "default": null,
                    "verbose_name": "Database Path",
                    "description": "If Path is Folder, the App will use the default DB Name"
                },
                "backup_database": {
                    "converter": "boolean",
                    "default": "False",
                    "implemented": false
                },
                "backup_limit": {
                    "converter": "integer",
                    "default": "0",
                    "implemented": false
                },
                "enable_connection_creation_logging": {
                    "converter": "boolean",
                    "default": "False"
                },
                "enable_profiling_hook_logging": {
                    "converter": "boolean",
                    "default": "False"
                },
                "enable_update_hook_logging": {
                    "converter": "boolean",
                    "default": "False"
                },
                "enable_rollback_hook_logging": {
                    "converter": "boolean",
                    "default": "False"
                },
                "enable_wal_hook_logging": {
                    "converter": "boolean",
                    "default": "False"
                }
            }
        },
        "updating": {
            "entries": {
                "updates_enabled": {
                    "converter": "boolean",
                    "default": "True",
                    "description": "Setting this to 'No' will disable updates globally, even for manual updates."
                },
                "update_interval": {
                    "converter": "timedelta",
                    "default": "5 min"
                },
                "max_update_time_frame": {
                    "converter": "timedelta",
                    "default": null,
                    "initial_value": "2 weeks",
                    "description": "Logs that were modified in this Timeframe will get updated in the App."
                },
                "remove_items_older_than_max_update_time_frame": {
                    "converter": "boolean",
                    "default": "False",
                    "description": "If set to 'Yes' will remove entries for items older than the max update time frame from the db (helps keep DB size small)"
                }
            }
        },
        "folder": {
            "entries": {
                "local_storage_folder": {
                    "converter": "path",
                    "default": null,
                    "gui_visible": false,
                    "description": "Where the app should download the log_file to when updating, defaults to Temp folder"
                }
            }
        },
        "downloading": {
            "entries": {
                "keep_downloaded_files": {
                    "converter": "boolean",
                    "default": "False",
                    "gui_visible": false,
                    "description": "If set to 'yes' the app will not delete the original log-files after finishing parsing."
                },
                "chunk_size": {
                    "converter": "file_size",
                    "default": null,
                    "initial_value": "5mb",
                    "implemented": false
                },
                "global_download_speed_limit": {
                    "converter": "file_size",
                    "default": "10mb",
                    "initial_value": "10mb"
                }
            }
        },
        "parsing": {
            "entries": {
                "record_insert_batch_size": {
                    "converter": "integer",
                    "default": "32767"
                }
            }
        },
        "time": {
            "entries": {
                "use_local_timezone": {
                    "converter": "boolean",
                    "default": "False"
                },
                "time_format": {
                    "converter": "string",
                    "default": "%Y-%m-%d %H:%M:%S.%f"
                }
            }
        },
        "gui": {
            "entries": {
                "style": {
                    "converter": "string",
                    "default": "base"
                },
                "font_family": {
                    "converter": "string",
                    "default": null,
                    "implemented": false
                },
                "font_size": {
                    "converter": "integer",
                    "default": null,
                    "implemented": false
                },
                "use_cat_busy_spinner": {
                    "converter": "boolean",
                    "default": "True",
                    "implemented": true,
                    "verbose_name": "Use Cat Busy-Spinner"
                },
                "notify_on_update_finished": {
                    "converter": "boolean",
                    "default": "False",
                    "implemented": true,
                    "verbose_name": "Send Windows Notification when update finishes"
                }
            }
        },
        "debug": {
            "entries": {
                "dev_mode": {
                    "converter": "boolean",
                    "default": "False"
                }
            }
        },
        "webdav": {
            "entries": {
                "max_concurrent_connections": {
                    "converter": "integer(minimum=1)",
                    "default": "1",
                    "initial_value": "5"
                }
            }
        },
        "logging": {
            "entries": {
                "level": {
                    "converter": "string",
                    "default": "debug",
                    "gui_visible": false
                },
                "max_function_name_length": {
                    "converter": "integer",
                    "default": "25",
                    "gui_visible": false
                },
                "max_module_name_length": {
                    "converter": "integer",
                    "default": "25",
                    "gui_visible": false
                }
            }
        },
        "mod_handling": {
            "entries": {
                "mod_set_exluded_mod_names": {
                    "converter": "list",
                    "default": "",
                    "gui_visible": false
                }
            }
        }
    }
}